<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.RNG">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WheelOfRNGActivity" android:theme="@style/WheelActionBarTheme" android:label="@string/wheel_of_rng" />
        <activity android:name=".DiceActivity" android:theme="@style/DiceActionBarTheme" android:label="@string/dice"/>
        <activity android:name=".RandomNumberActivity" android:theme="@style/RandomNumberActionBarTheme" android:label="@string/random_number"/>
        <activity android:name=".RandomPasswordActivity" android:theme="@style/RandomPasswordActionBarTheme" android:label="@string/random_password"></activity>
    </application>

</manifest>